					.phone-pop-wrap.calculator-phone-pop-wrap
						.phone-pop
							span.close-phone-pop X
							input(
								name="name"
								type="text"
								placeholder='Имя'
								required
							).calculator-name
							input(
								name="tel",
								type="tel",
								placeholder="Телефон"
								autocorrect="off"
								autocomplete="tel"
								required)#phone-id.calculator-phone
							input(
								name="address"
								type="text"
								placeholder='Адрес проживания'
								autocorrect="off"
								autocomplete="address-line1"
								required
							).calculator-address
							input.btn.calculator-submit(
								type="submit"
								value="Отправить")
					.calculator-cost-wrap
						.calculator-cost
							input(
								name="calculatorCost"
								type="text"
								disabled
